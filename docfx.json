{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.cs"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "exclude": [
          "_site/**",
          "obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SDSL Documentation",
      "_appName": "SDSL Docs",
      "_appFooter": "SDSL - The most elegant shading language",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/tebjan/SDSL-docs",
        "branch": "main"
      }
    }
  }
}
